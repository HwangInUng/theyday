generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id       Int    @id @default(autoincrement())
  name     String
  price    Int    @default(0)
  diameter Int    @default(0)
  height   Int    @default(0)
  option1  String
  option2  String
}

